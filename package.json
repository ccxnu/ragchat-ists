{
  "name": "app-ragchat-ists",
  "version": "0.0.1",
  "description": "A RAG chatbot for ISTS",
  "author": "Pablo Cuenca",
  "private": true,
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "nest start --watch",
    "debug": "nest start --watch --debug=0.0.0.0:9229",
    "migration:run": "ts-node src/infra/database/migrations/index.ts",
    "migration:down": "ts-node src/infra/database/migrations/migrator-down.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build:clean": "rimraf dist; exit 0",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestia/core": "4.4.0",
    "@nestia/e2e": "0.7.0",
    "@nestia/fetcher": "4.4.0",
    "@nestia/sdk": "4.4.0",
    "@nestjs/axios": "3.1.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/swagger": "8.1.0",
    "axios": "1.7.7",
    "bcryptjs": "2.4.3",
    "handlebars": "4.7.8",
    "html-minifier-terser": "7.2.0",
    "ioredis": "5.4.1",
    "kysely": "0.27.4",
    "nestjs-zod": "4.2.0",
    "ollama": "0.5.11",
    "openai": "4.80.0",
    "passport-jwt": "4.0.1",
    "pg": "8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typia": "7.4.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "9.19.0",
    "@nestia/benchmark": "0.3.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/html-minifier-terser": "7.0.2",
    "@types/node": "^20.16.5",
    "@types/passport-jwt": "4.0.1",
    "@types/pg": "8.11.10",
    "@types/supertest": "^6.0.0",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nestia": "6.3.1",
    "prettier": "3.4.2",
    "prettier-plugin-brace-style": "0.7.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "10.9.2",
    "ts-patch": "3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.23.0"
  }
}
